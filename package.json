{
  "name": "vscode-st",
  "displayName": "Structured Text language Support",
  "description": "Supports for syntax highlights, snippets for IEC 61131-3 Structured Text",
  "version": "1.0.8",
  "publisher": "Serhioromano",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Serhioromano/vscode-st.git"
  },
  "keywords": [
    "ST",
    "PLC",
    "logi.CAD 3",
    "IEC 61131-3",
    "Structured Text"
  ],
  "author": "Sergey Romanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Serhioromano/vscode-st/issues",
    "email": "serg4172@mail.ru"
  },
  "homepage": "https://github.com/Serhioromano/vscode-st#readme",
  "engines": {
    "vscode": "^1.20.0"
  },
  "activationEvents": [
    "onLanguage:st"
  ],
  "main": "index.js",
  "categories": [
    "Languages",
    "Snippets"
  ],
  "icon": "st-icon.png",
  "contributes": {
    "languages": [
      {
        "id": "st",
        "aliases": [
          "Structured Text",
          "ST"
        ],
        "firstLine": "^([\\(\\/\\*]?.*)?(PROGRAM ).*",
        "extensions": [
          ".st",
          ".stl",
          ".scl",
          ".iecplc",
          ".iecst",
          ".iecstl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "st",
        "scopeName": "source.st",
        "path": "./syntaxes/st.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "st",
        "path": "./snippets/st.snippets.json"
      }
    ]
  }
}